//User function Template for C++

class Solution{
public:
    long long findNumber(long long N){
        long long curr = 1 , ans = 0;
        int arr[] = { 9 , 1 , 3 , 5 , 7 };
        while(N){
            ans = arr[N%5] * curr + ans;
            if(N%5 == 0){
                N = N/5 - 1;
            }else{
                N = N/5;
            }
            curr = curr * 10;
        }
        return ans;
    }
};
